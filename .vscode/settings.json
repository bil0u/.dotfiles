{
    "files.associations": {
        "*.yml": "yaml",
        "*.tmpl": "go-template",
        "*vimrc*": "plaintext",
        "*zshrc*": "shellscript",
        "*.zsh*": "shellscript",
        "*.sh": "shellscript",
        "*.ps1": "powershell",
        "*ignore": "ignore",
        "*gitconfig*": "ini",
        // "**/.chezmoiexternal.yaml": "go-template",
        "**/.chezmoiscripts/darwin/**": "shellscript",
        "**/.chezmoiscripts/windows/**": "powershell",
        "**/.chezmoitemplates/**": "go-template",
        "**/.chezmoitemplates/darwin/**": "shellscript",
        "**/.chezmoitemplates/windows/**": "powershell",
    },
    "powershell.codeFormatting.whitespaceBeforeOpenBrace": false,
    "powershell.codeFormatting.whitespaceInsideBrace": false,
    "powershell.codeFormatting.whitespaceAroundOperator": false,
    "powershell.codeFormatting.newLineAfterCloseBrace": false,
    "powershell.codeFormatting.newLineAfterOpenBrace": false,

}